<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Backwards compatibility patch for RimWorld 1.2, which re-adds an XML field made obsolete in the 1.3 update -->
	
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="Hamefura_Apparel_PjEggplant_Hood"]
				</xpath>
				<value>
					<menuHidden>true</menuHidden>
				</value>
			</li>
		</operations>
	</Operation>

</Patch>